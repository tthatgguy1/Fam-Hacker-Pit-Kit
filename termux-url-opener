#!/data/data/com.termux/files/usr/bin/bash
# Project name : Fam-Hacker-Pit-Kit
# Coded by: tthatgguy1 (You dont become a coder by just changing the credits
# Github: https://github.com/tthatgguy1/Fam-Hacker-Pit-Kit
# Date : 12/04/2024
# Updated 09/09/2025
URL="$1"

                 # <â”€â”€â”€â”€â”€ Directory Escapes â”€â”€â”€â”€â”€> #
NFS_HOME="$HOME/storage/shared/Fam-Hacker-Pit-Kit" # ${NFS_HOME}
YT_VIDEO_PIT="$HOME/storage/shared/Fam-Hacker-Pit-Kit/YouTube-Video-Pit" # ${YT_VIDEO_PIT}
YT_AUDIO_PIT="$HOME/storage/shared/Fam-Hacker-Pit-Kit/YouTube-Audio-Pit" # ${YT_AUDIO_PIT}
YouTube_Work="$HOME/downloads" # ${YouTube_Work}

                 # <â”€â”€â”€â”€â”€ Sound Effect Escsapes â”€â”€â”€â”€â”€> #
TXS="/.NFS-Data/sfx/TXS.mp3"        # ${TXS}
MSTART="/.NFS-Data/sfx/MDLD.mp3"    # ${MSTART}
CMP="/.NFS-Data/sfx/c.mp3"          # ${CMP}
MDATA="/.NFS-Data/sfx/MDATA.mp3"    # ${MDATA}
FHS="/.NFS-Data/sfx/FHS.mp3"        # ${FHS}
YTSU="/.NFS-Data/sfx/YTSU.mp3"      # ${YTSU}
TB="/.NFS-Data/sfx/TB.mp3"          # ${TB}
MFGTA="/.NFS-Data/sfx/MFGTA.mp3"    # ${MFGTA}
SKL="/.NFS-Data/sfx/SKL.mp3"        # ${SKL}
MUSIC_SLAVE="mpv"                   # ${MUSIC_SLAVE}

                   # <â”€â”€â”€â”€â”€ Color Setup â”€â”€â”€â”€â”€> #
ESC=$'\e'
GRN="${ESC}[38;2;51;255;0m"       # ${GRN}   $GRN  - Lime Green
PNK="${ESC}[38;2;255;16;240m"     # ${PNK}   $PNK  - Bright Magenta
BLU="${ESC}[38;2;0;102;255m"      # ${BLU}   $BLU  - Medium Blue
BONG="${ESC}[38;2;255;140;0m"     # ${BONG}  $BONG - Pumpkin Orange
NONG="${ESC}[38;2;255;95;0m"      # ${NONG}  $NONG - Neon Orange

BCYN="${ESC}[38;2;0;255;255m"     # ${BCYN}  $BCYN - Cyan
ACID="${ESC}[38;2;204;255;0m"     # ${ACID}  $ACID - Acid Green
VBLU="${ESC}[38;2;138;43;226m"    # ${VBLU}  $VBLU - Violet Blue
NPPL="${ESC}[38;2;188;19;254m"    # ${NPPL}  $NPPL - Neon Purple
BWHT="${ESC}[38;2;255;255;255m"   # ${BWHT}  #BWHT - Bright White
RESET="${ESC}[0m"                 # ${RESET} $RESET

		    # <â”€â”€â”€â”€â”€ Redirect Termux â”€â”€â”€â”€â”€> #
cd $YouTube_Work
		     # <â”€â”€â”€â”€â”€ Intro Banner â”€â”€â”€â”€â”€> #
echo " "
toilet -k -o -f smslant -F metal "Fam-Hacker"
echo " "
  cowsay -r "Welcome To Fam-Hacker Tube Client, Written By Nah-Fam_Studios (Seth J. Nelson). This Script Will Download Any Shared YouTube URL to Termux." | lolcat --seed=53
    "${MUSIC_SLAVE}" "${NFS_HOME}/${SKL}" >/dev/null 2>&1
#    "${MUSIC_SLAVE}" "${NFS_HOME}/${YTSU}" >/dev/null 2>&1 # 57
sleep 2.0
    "${MUSIC_SLAVE}" "${NFS_HOME}/${MDATA}" >/dev/null 2>&1

		    # <â”€â”€â”€â”€â”€ Channel Strings â”€â”€â”€â”€â”€> #
DELIM=$'\x1f'
FMT=$'%(title)s\x1f%(uploader)s\x1f%(channel_follower_count)s\x1f%(like_count)s\x1f%(upload_date)s\x1f%(width)s\x1f%(height)s\x1f%(playlist_title)s'
IFS=$DELIM read -r TITLE CHANNEL SUBS LIKES UPLOAD_DATE QLTYW QLTYH PLYLST < <(
  yt-dlp -q --no-warnings --print "$FMT" "$URL"
)
	       # <â”€â”€â”€â”€â”€ Upload Date Format To US Engish â”€â”€â”€â”€â”€> #
if [[ "$UPLOAD_DATE" =~ ^[0-9]{8}$ ]]; then
  UPLOAD_DATE_FMT="${BCYN}${UPLOAD_DATE:4:2}${RESET}${PNK}-${RESET}${BCYN}${UPLOAD_DATE:6:2}${RESET}${PNK}-${RESET}${BCYN}${UPLOAD_DATE:0:4}${RESET}"
else
  UPLOAD_DATE_FMT="${UPLOAD_DATE}"
fi

                    # <â”€â”€â”€â”€â”€ Download Options â”€â”€â”€â”€â”€> #
OPTION_BEST_VIDEO_MKV="${VBLU}Download${RESET} \
${BCYN}Video${RESET} \
${VBLU}In the${RESET} \
${BCYN}Best${RESET} \
${VBLU}Quality${RESET}\
${PNK}.${RESET}"

OPTION_1440_MKV="${VBLU}Download${RESET} \
${BCYN}Video${RESET} \
${VBLU}In${RESET} \
${BCYN}1440p${RESET} \
${PNK}+${RESET} \
${BCYN}Best${RESET} \
${VBLU}Audio${RESET}\
${PNK}.${RESET}"

OPTION_1080_MKV="${VBLU}Download${RESET} \
${BCYN}Video${RESET} \
${VBLU}In${RESET} \
${BCYN}1080p${RESET} \
${PNK}+${RESET} \
${BCYN}Best${RESET} \
${VBLU}Audio${RESET}\
${PNK}.${RESET}"

OPTION_BEST_AUDIO_MP3="${VBLU}Extract${RESET} \
${PNK}&${RESET} \
${VBLU}Download${RESET} \
${VBLU}An${RESET} \
${BCYN}Audio${RESET} \
${PNK}MP3${RESET} \
${VBLU}File${RESET}\
${PNK}.${RESET}"

OPTION_MP3_PLAYLIST="${VBLU}Extract${RESET} \
${PNK}&${RESET} \
${VBLU}Download${RESET} \
${VBLU}An${RESET} \
${BCYN}Entire${RESET} \
${PNK}MP3${RESET} \
${BCYN}Audio${RESET} \
${VBLU}Playlist${RESET}\
${PNK}.${RESET}"

OPTION_QUIT="${BWHT}[${NONG}Quit${BWHT}]${RESET}"

                 # <â”€â”€â”€â”€â”€ Download Options Comand â”€â”€â”€â”€â”€> #
OPT_BEST_VIDEO_MKV_CMD=(
  yt-dlp
   -f "bestvideo+bestaudio"
   -o "${YouTube_Work}/%(uploader)s - %(title)s [%(height)sp]{Best-Resolution+Audio}.mkv"
	--progress --quiet
	--console-title --no-warnings
	--merge-output-format mkv
	--embed-thumbnail --embed-chapters
	--embed-subs --no-check-formats
	--progress-template "download: ${PNK}DL${BCYN}:%(progress._percent_str)s ${PNK}ETA${BCYN}: %(progress._eta_str)s ${PNK}Speed${BCYN}:%(progress._speed_str)s"
	  "$URL")

OPT_1440_MKV_CMD=(
  yt-dlp
    -f "bestvideo[height<=1440]+bestaudio/best[height<=1440]"
    -o "${YouTube_Work}/%(uploader)s - %(title)s [%(height)sp].mkv"
	--progress --quiet
	--console-title --no-warnings
	--merge-output-format mkv
	--embed-thumbnail --embed-chapters
	--embed-subs --no-check-formats
	--progress-template "download: ${PNK}DL${BCYN}:%(progress._percent_str)s ${PNK}ETA${BCYN}: %(progress._eta_str)s ${PNK}Speed${BCYN}:%(progress._speed_str)s"
	 "$URL")

OPT_1080_MKV_CMD=(
  yt-dlp
    -f "bestvideo[height<=1080]+bestaudio/best[height<=1080]"
    -o "${YouTube_Work}/%(uploader)s - %(title)s [%(height)sp].mkv"
	--progress --quiet
	--console-title --no-warnings
	--merge-output-format mkv
	--embed-thumbnail --embed-chapters
	--embed-subs --no-check-formats
	--progress-template "download: ${PNK}DL${BCYN}:%(progress._percent_str)s ${PNK}ETA${BCYN}: %(progress._eta_str)s ${PNK}Speed${BCYN}:%(progress._speed_str)s"
	 "$URL")

OPT_BEST_AUDIO_MP3_CMD=(
  yt-dlp
    -f "bestaudio"
    -x --audio-format mp3
    -o "${YouTube_Work}/%(uploader)s - %(title)s.mp3"
	--progress --quiet
	--console-title --no-warnings
	--no-check-formats
	--progress-template "download: ${PNK}DL${BCYN}:%(progress._percent_str)s ${PNK}ETA${BCYN}: %(progress._eta_str)s ${PNK}Speed${BCYN}:%(progress._speed_str)s"
	"$URL")

OPT_PLAYLIST_MP3_CMD=(
  yt-dlp
    -f "bestaudio"
    -x --audio-format mp3
    -o "${YouTube_Work}/%(playlist_title)s/%(uploader)s - %(title)s.mp3"
	--progress --quiet
	--console-title --no-warnings
	--no-check-formats
	--progress-template "download: ${PNK}DL${BCYN}:%(progress._percent_str)s ${PNK}ETA${BCYN}: %(progress._eta_str)s ${PNK}Speed${BCYN}:%(progress._speed_str)s"
	"$URL")

	         # <â”€â”€â”€â”€â”€ Default Option With Timeout â”€â”€â”€â”€â”€> #
DEFAULT="5"

		       # <â”€â”€â”€â”€â”€ Decision Time â”€â”€â”€â”€â”€> #
  clear
    echo " "
    toilet -f smslant -F gay "DL Options"
    cowsay -f stan "${TITLE}" | lolcat --seed=53
      echo " "
      echo " "
      echo " "
      printf "${VBLU}â”Œâ”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”\n${RESET}"
      printf "${VBLU}â”‚ ${PNK}[${BCYN}1${PNK}] %s      ${VBLU}â”‚\n" "$OPTION_BEST_VIDEO_MKV"
      printf "${VBLU}â”œâ”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”¤\n"
      printf "${VBLU}â”‚ ${PNK}[${BCYN}2${PNK}] %s    ${VBLU}â”‚\n" "$OPTION_1440_MKV"
      printf "${VBLU}â”œâ”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”¤\n"
      printf "${VBLU}â”‚ ${PNK}[${BCYN}3${PNK}] %s    ${VBLU}â”‚\n" "$OPTION_1080_MKV"
      printf "${VBLU}â”œâ”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”¤\n"
      printf "${VBLU}â”‚ ${PNK}[${BCYN}4${PNK}] %s    ${VBLU}â”‚\n" "$OPTION_BEST_AUDIO_MP3"
      printf "${VBLU}â”œâ”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”¤\n"
      printf "${VBLU}â”‚ ${PNK}[${BCYN}5${PNK}] %s    ${VBLU}â”‚\n" "$OPTION_MP3_PLAYLIST"
      printf "${VBLU}â”œâ”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”¤\n"
      printf "${VBLU}â”‚ ${ACID}[${PNK}6${ACID}] %s                                   ${VBLU}â”‚\n" "$OPTION_QUIT"
      printf "${VBLU}â”œâ”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”¤\n"
      printf "${VBLU}â”‚ ${PNK}[${BCYN}E${PNK}] ${VBLU}Press ${BWHT}[${NONG}Enter${BWHT}] ${VBLU}For Default Option ${BWHT}[${NONG}%d${BWHT}]${RESET}     ${VBLU}â”‚\n" "$DEFAULT"
      printf "${VBLU}â””â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”˜\n"
      echo " "
      echo " "
    printf "${VBLU}â”Œâ”€â”€${BCYN}Please${VBLU}â”€${BCYN}Make${VBLU}â”€${PNK}Any${VBLU}â”€${BCYN}Decision${VBLU}â”€â”€â”${RESET}\n${VBLU}â””â”€${BCYN}>${PNK}: "
    read -r -t120 DECISION
    clear
    echo " "
  DECISION=${DECISION:-$DEFAULT}
  rc=2
  case "${DECISION}" in
	      # <â”€â”€â”€â”€â”€ Choice Number 1) Best Video and Audio â”€â”€â”€â”€â”€> #
    1)
      date +"%l:%M:%S %p" | lolcat --seed=52
      "${MUSIC_SLAVE}" "${NFS_HOME}/${MSTART}" >/dev/null 2>&1
       cowsay -r "${TITLE}" | lolcat --seed=53
        echo " "
        printf    "${VBLU}â”Œâ”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”\n${RESET}"
        printf    "${VBLU}â”œâ”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€ğŸ‘½â”€${PNK}[${BCYN}Nah${VBLU}â”€${BCYN}Fam${VBLU}â”€${BCYN}Studios${PNK}]${VBLU}â”€ğŸ‘½â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€${VBLU}â”¤\n${RESET}"
        printf    "${VBLU}â”œâ”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”¤\n${RESET}"
        printf    "${VBLU}â”‚  ${NONG}â”€â”€${BWHT}> ğŸ“º    ${PNK}Chl${BCYN}:  	${PNK}[${BCYN}${CHANNEL}${PNK}]${RESET}\n"
        printf    "${VBLU}â”‚  ${NONG}â”€-${BWHT}> ğŸ™‹    ${PNK}Sbs${BCYN}:  	${PNK}[${BCYN}${SUBS}${PNK}]${RESET}\n"
        printf    "${VBLU}â”‚  ${NONG}â”€â”€${BWHT}> ğŸ‘    ${PNK}Lks${BCYN}:  	${PNK}[${BCYN}${LIKES}${PNK}]${RESET}\n"
        printf    "${VBLU}â”‚  ${NONG}â”€-${BWHT}> ğŸ“¹    ${PNK}Res${BCYN}:  	${PNK}[${BCYN}${QLTYW}${PNK}x${BCYN}${QLTYH}${PNK}]${RESET}\n"
        printf    "${VBLU}â”‚  ${NONG}â”€â”€${BWHT}> ğŸ“†    ${PNK}Upd${BCYN}:  	${PNK}[${BCYN}${UPLOAD_DATE_FMT}${RESET}${PNK}]${RESET}\n"
        printf    "${VBLU}â””â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”˜${RESET}"
        echo " "
        echo " "
        printf "       ${VBLU}â”Œâ”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”\n"
        printf " ${VBLU}â”€â”€â”€â”€â”€â”€â”¤  ${BWHT}[${NONG}Running${BWHT}]${GRN}: ${PNK}Best ${BCYN}Video${PNK}+${BCYN}Audio${VBLU}  â”œâ”€â”€â”€â”€â”€â”€â”€${RESET}\n"
        printf "       ${VBLU}â””â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”˜\n"
	sleep 2.0
                    # <â”€â”€â”€â”€â”€ Actual YT-DLP Command â”€â”€â”€â”€â”€> #
	"${OPT_BEST_VIDEO_MKV_CMD[@]}";
       rc=$?
                  # <â”€â”€â”€â”€â”€ Fail Safe Download Status â”€â”€â”€â”€â”€> #
    if [ $rc -eq 0 ]; then
      "${MUSIC_SLAVE}" "${NFS_HOME}/${CMP}" >/dev/null 2>&1
       "${MUSIC_SLAVE}" "${NFS_HOME}/${FHS}" >/dev/null 2>&1
      cowsay -r "Your MKV File Has Downloaded Successfully!" | lolcat --seed=36
	sleep 2.0
       "${MUSIC_SLAVE}" "${NFS_HOME}/${TXS}" >/dev/null 2>&1
	clear
    else
      "${MUSIC_SLAVE}" "${NFS_HOME}/${MFGTA}" >/dev/null 2>&1
      cowsay -r "Your Video Download Has Failed, Please Try Again." | lolcat --seed=25
      printf "${NONG}â”Œâ”€â”€${BWHT}Please${NONG}â”€${BWHT}Press${NONG}â”€${BWHT}Any${NONG}â”€${BWHT}Button${NONG}â”€â”€â”${RESET}\n${NONG}â””â”€${GRN}>${BWHT}: "
	read -n1 -r -t3
	clear
    fi
                # <â”€â”€â”€â”€â”€ Fail Safe File Transfer Status â”€â”€â”€â”€â”€> #
newf=$(ls -t -- *.mkv 2>/dev/null | head -n1)
if [ -n "$newf" ]; then
  mv -- "$newf" "$YT_VIDEO_PIT" >/dev/null 2>&1
    cowsay -f cube "Your MKV File Has Been Transferred To YT_VIDEO_PIT, Successfully!" | lolcat --seed=56
     "${MUSIC_SLAVE}" "${NFS_HOME}/${FHS}" >/dev/null 2>&1
    sleep 1.0
    printf "${VBLU}â”Œâ”€â”€${BCYN}Please${VBLU}â”€${BCYN}Press${VBLU}â”€${PNK}Any${VBLU}â”€${BCYN}Button${VBLU}â”€â”€â”${RESET}\n${VBLU}â””â”€${BCYN}>${PNK}: "
  read -n1 -r -t3
else
     "${MUSIC_SLAVE}" "${NFS_HOME}/${MFGTA}" >/dev/null 2>&1
   cowsay -r "Your MKV File Has Failed To Transfer" | lolcat --seed=25
   echo " "
   printf "${BWHT}â”Œâ”€â”€${NONG}Please${BWHT}â”€${NONG}Press${BWHT}â”€${NONG}Any${BWHT}â”€${NONG}Button${BWHT}â”€â”€â”${RESET}\n${BWHT}â””â”€${NONG}>${GRN}: "
  read -n1 -r -t5
fi
;;
            # <â”€â”€â”€â”€â”€ Choice Number 2) 1440p Video and Audio â”€â”€â”€â”€â”€> #
    2)
      date +"%l:%M:%S %p" | lolcat --seed=52
      "${MUSIC_SLAVE}" "${NFS_HOME}/${MSTART}" >/dev/null 2>&1
        echo " "
       cowsay -r "${TITLE}" | lolcat --seed=53
        echo " "
        printf    "${VBLU}â”Œâ”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”\n${RESET}"
