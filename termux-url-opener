  read -n1 -r -t3
else
     "${MUSIC_SLAVE}" "${NFS_HOME}/${MFGTA}" >/dev/null 2>&1
    cowsay -r "Your MP3 File Has Failed To Transfer" | lolcat --seed=25
    printf "${VBLU}‚îå‚îÄ‚îÄ${BCYN}Please${VBLU}‚îÄ${BCYN}Press${VBLU}‚îÄ${PNK}Any${VBLU}‚îÄ${BCYN}Button${VBLU}‚îÄ‚îÄ‚îê${RESET}\n${VBLU}‚îî‚îÄ${BCYN}>${PNK}: "
    read -n1 -r -t3
fi
;;
  5)
              # <‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ Choice Number 5) Playlist Audio .mp3 ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ> #
   date +"%l:%M:%S %p" | lolcat --seed=52
   echo " "
   cowsay -r "${TITLE}" | lolcat --seed=53
      echo " "
      printf    "${VBLU}‚îå‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îê\n${RESET}"
      printf    "${VBLU}‚îú‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄüëΩ‚îÄ${PNK}[${BCYN}Nah${VBLU}‚îÄ${BCYN}Fam${VBLU}‚îÄ${BCYN}Studios${PNK}]${VBLU}‚îÄüëΩ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ${VBLU}‚î§\n${RESET}"
      printf    "${VBLU}‚îú‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚î§\n${RESET}"
      printf    "${VBLU}‚îÇ  ${BCYN}‚îÄ‚îÄ${PNK}> üì∫    ${PNK}Chl${BCYN}:  	${PNK}[${BCYN}${CHANNEL}${PNK}]${RESET}\n"
      printf    "${VBLU}‚îÇ  ${BCYN}‚îÄ-${PNK}> üôã    ${PNK}Sbs${BCYN}:  	${PNK}[${BCYN}${SUBS}${PNK}]${RESET}\n"
      printf    "${VBLU}‚îÇ  ${BCYN}‚îÄ‚îÄ${PNK}> üëç    ${PNK}Lks${BCYN}:  	${PNK}[${BCYN}${LIKES}${PNK}]${RESET}\n"
      printf    "${VBLU}‚îÇ  ${BCYN}‚îÄ‚îÄ${PNK}> üëç    ${PNK}Pyl${BCYN}:  	${PNK}[${BCYN}${PLYLST}${PNK}]${RESET}\n"
      printf    "${VBLU}‚îÇ  ${BCYN}‚îÄ‚îÄ${PNK}> üìÜ    ${PNK}Upd${BCYN}:  	${PNK}[${BCYN}${UPLOAD_DATE_FMT}${RESET}${PNK}]${RESET}\n"
      printf    "${VBLU}‚îî‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îò${RESET}"
      echo " "
      echo " "
      printf "       ${VBLU}‚îå‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îê\n"
      printf " ${VBLU}‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚î§  ${BWHT}[${NONG}Running${BWHT}]${GRN}: ${PNK}Playlist ${BCYN}Audio${VBLU}    ‚îú‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ${RESET}\n"
      printf "       ${VBLU}‚îî‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îò\n"
      sleep 2.0
    "${OPT_PLAYLIST_MP3_CMD[@]}";
    rc=$?
              # <‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ Fail Safe Audio Playlist Download Status ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ> #
    if [ $rc -eq 0 ]; then
      "${MUSIC_SLAVE}" "${NFS_HOME}/${CMP}" >/dev/null 2>&1
       "${MUSIC_SLAVE}" "${NFS_HOME}/${FHS}" >/dev/null 2>&1
      cowsay -r "Your MP3 Playlist Has Downloaded Successfully!" | lolcat --seed=53
	sleep 2.0
       "${MUSIC_SLAVE}" "${NFS_HOME}/${TXS}" >/dev/null 2>&1
	clear
    else
      "${MUSIC_SLAVE}" "${NFS_HOME}/${MFGTA}" >/dev/null 2>&1
      cowsay -r "Your Audio Playlist Extract Has Failed, Please Try Again." | lolcat --seed=25
      printf "${BCYN}‚îå‚îÄ‚îÄ${PNK}Please${BCYN}‚îÄ${PNK}Press${BCYN}‚îÄ${PNK}Any${BCYN}‚îÄ${PNK}Button${BCYN}‚îÄ‚îÄ‚îê${RESET}\n${BCYN}‚îî‚îÄ${PNK}>${PNK}: "
	read -n1 -r
	clear
    fi
                # <‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ Fail Safe File Transfer Status ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ> #
newf=$(ls -t -- * 2>/dev/null | head -n1)
if [ -n "$newf" ]; then
  mv -- "$newf" "$YT_AUDIO_PIT" >/dev/null 2>&1
    cowsay -f cube "Your MP3 Playlist Has Been Transferred To YT_VIDEO_PIT, Successfully!" | lolcat --seed=53
     "${MUSIC_SLAVE}" "${NFS_HOME}/${FHS}" >/dev/null 2>&1
    sleep 1.0
    printf "${BCYN}‚îå‚îÄ‚îÄ${PNK}Please${BCYN}‚îÄ${PNK}Press${BCYN}‚îÄ${PNK}Any${BCYN}‚îÄ${PNK}Button${BCYN}‚îÄ‚îÄ‚îê${RESET}\n${BCYN}‚îî‚îÄ${PNK}>${PNK}: "
  read -n1 -r -t3
else
     "${MUSIC_SLAVE}" "${NFS_HOME}/${MFGTA}" >/dev/null 2>&1
    cowsay -r "Your MP3 Playlist Has Failed To Transfer" | lolcat --seed=25
    printf "${VBLU}‚îå‚îÄ‚îÄ${BCYN}Please${VBLU}‚îÄ${BCYN}Press${VBLU}‚îÄ${PNK}Any${VBLU}‚îÄ${BCYN}Button${VBLU}‚îÄ‚îÄ‚îê${RESET}\n${VBLU}‚îî‚îÄ${BCYN}>${PNK}: "
    read -n1 -r -t3
fi
;;
  6)
   rc=2
     exit
;;
  *)
    rc=2
      printf "${BCYN}‚îå‚îÄ‚îÄ${PNK}Please${BCYN}‚îÄ${PNK}Press${BCYN}‚îÄ${PNK}Any${BCYN}‚îÄ${PNK}Button${BCYN}‚îÄ‚îÄ‚îê${RESET}\n ${BCYN}‚îî‚îÄ${PNK}>${PNK}: "
      read -r -t60
;;
esac
