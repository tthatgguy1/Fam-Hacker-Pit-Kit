    clear
  fi
newd=$(ls -td -- */ 2>/dev/null | head -n1)
  if [ -n "$newd" ]; then
    mv -- "$newd" "$APIT" >/dev/null 2>&1
    cowsay -r "Your MP3 Playlist Has Been Transferred To Audio_PIT Successfully"
    mpv "$HOME/storage/shared/Fam-Hacker-Pit-Kit/.NFS-Data/sfx/FHS.mp3" >/dev/null 2>&1
    sleep 1.0
    printf "\n"
    printf "â”Œâ”€â”€Pressâ”€Anyâ”€Buttonâ”€Toâ”€Moveâ”€Onâ”€â”€â”\n"
