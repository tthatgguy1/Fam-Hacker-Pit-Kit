              # <â”€â”€â”€â”€â”€ Fail Safe Audio Playlist Download Status â”€â”€â”€â”€â”€> #
    if [ $rc -eq 0 ]; then
      "${MUSIC_SLAVE}" "${NFS_HOME}/${CMP}" >/dev/null 2>&1
       "${MUSIC_SLAVE}" "${NFS_HOME}/${FHS}" >/dev/null 2>&1
      cowsay -r "Your MP3 Playlist Has Downloaded Successfully!" | lolcat --seed=53
	sleep 2.0
       "${MUSIC_SLAVE}" "${NFS_HOME}/${TXS}" >/dev/null 2>&1
	clear
    else
      "${MUSIC_SLAVE}" "${NFS_HOME}/${MFGTA}" >/dev/null 2>&1
      cowsay -r "Your Audio Playlist Extract Has Failed, Please Try Again." | lolcat --seed=25
      printf "${BCYN}â”Œâ”€â”€${PNK}Please${BCYN}â”€${PNK}Press${BCYN}â”€${PNK}Any${BCYN}â”€${PNK}Button${BCYN}â”€â”€â”${RESET}\n${BCYN}â””â”€${PNK}>${PNK}: "
	read -n1 -r
	clear
    fi
                # <â”€â”€â”€â”€â”€ Fail Safe File Transfer Status â”€â”€â”€â”€â”€> #
newf=$(ls -t -- * 2>/dev/null | head -n1)
if [ -n "$newf" ]; then
  mv -- "$newf" "$YT_AUDIO_PIT" >/dev/null 2>&1
    cowsay -f cube "Your MP3 Playlist Has Been Transferred To YT_VIDEO_PIT, Successfully!" | lolcat --seed=53
     "${MUSIC_SLAVE}" "${NFS_HOME}/${FHS}" >/dev/null 2>&1
    sleep 1.0
    printf "${BCYN}â”Œâ”€â”€${PNK}Please${BCYN}â”€${PNK}Press${BCYN}â”€${PNK}Any${BCYN}â”€${PNK}Button${BCYN}â”€â”€â”${RESET}\n${BCYN}â””â”€${PNK}>${PNK}: "
  read -n1 -r -t3
else
     "${MUSIC_SLAVE}" "${NFS_HOME}/${MFGTA}" >/dev/null 2>&1
    cowsay -r "Your MP3 Playlist Has Failed To Transfer" | lolcat --seed=25
    printf "${VBLU}â”Œâ”€â”€${BCYN}Please${VBLU}â”€${BCYN}Press${VBLU}â”€${PNK}Any${VBLU}â”€${BCYN}Button${VBLU}â”€â”€â”${RESET}\n${VBLU}â””â”€${BCYN}>${PNK}: "
    read -n1 -r -t3
fi
;;
  6)
   rc=2
     exit
;;
  *)
    rc=2
      printf "${BCYN}â”Œâ”€â”€${PNK}Please${BCYN}â”€${PNK}Press${BCYN}â”€${PNK}Any${BCYN}â”€${PNK}Button${BCYN}â”€â”€â”${RESET}\n ${BCYN}â””â”€${PNK}>${PNK}: "
      read -r -t60
;;
esac
