#!/data/data/com.termux/files/usr/bin/bash
# Project Name: Fam-Hacker-Pit-Kit
# Coded by: tthatgguy1 (You dont become a coder by just changing the credits
# Github: https://github.com/tthatgguy1/Fam-Hacker-Pit-Kit
# Date : 12/04/2024
# Updated 09/12/2025

URL="$1"

ESC=$'\e'
NGRN="${ESC}[38;2;51;255;0m"                                            # ${NGRN}        $NGRN      - Lime Green
BMGT="${ESC}[38;2;255;16;240m"                                          # ${BMGT}        $BMGT      - Bright Magenta
NONG="${ESC}[38;2;255;95;0m"                                            # ${NONG}        $NONG      - Neon Orange
BCYN="${ESC}[38;2;0;255;255m"                                           # ${BCYN}        $BCYN      - Cyan
ACID="${ESC}[38;2;204;255;0m"                                           # ${ACID}        $ACID      - Acid Green
VBLU="${ESC}[38;2;138;43;226m"                                          # ${VBLU}        $VBLU      - Violet Blue
MMDB="${ESC}[38;2;25;25;112m"                                           # ${MMDB}        $MMDB      - Midnight Blue
NPPL="${ESC}[38;2;188;19;254m"                                          # ${NPPL}        $NPPL      - Neon Purple
BWHT="${ESC}[38;2;255;255;255m"                                         # ${BWHT}        #BWHT      - Bright White
RESET="${ESC}[0m"                                                       # ${RESET}       $RESET

VPIT="$HOME/storage/shared/Fam-Hacker-Pit-Kit/YouTube-Video-Pit"            # ${VPIT}
APIT="$HOME/storage/shared/Fam-Hacker-Pit-Kit/YouTube-Audio-Pit"            # ${APIT}

TXS=".NFS-Data/sfx/TXS.mp3"                                            # ${TXS}
MSTART=".NFS-Data/sfx/MDLD.mp3"                                        # ${MSTART}
CMP=".NFS-Data/sfx/c.mp3"                                              # ${CMP}
MDATA=".NFS-Data/sfx/MDATA.mp3"                                        # ${MDATA}
FHS=".NFS-Data/sfx/FHS.mp3"                                            # ${FHS}
MFGTA=".NFS-Data/sfx/MFGTA.mp3"                                        # ${MFGTA}
SKL=".NFS-Data/sfx/SKL.mp3"                                            # ${SKL}

cd downloads
printf "\n\n"

mpv "$HOME/storage/shared/Fam-Hacker-Pit-Kit/.NFS-Data/sfx/SKL.mp3" >/dev/null 2>&1
mpv "$HOME/storage/shared/Fam-Hacker-Pit-Kit/.NFS-Data/sfx/MFGTA.mp3" >/dev/null 2>&1
printf "\n\n"

DELIM=$'\x1f'
FMT=$'%(playlist_title)s\x1f%(title)s\x1f%(uploader)s\x1f%(channel_follower_count)s\x1f%(like_count)s\x1f%(upload_date)s\x1f%(width)s\x1f%(height)s'
IFS=$DELIM read -r PTITLE TITLE CHANNEL SUBS LIKES UPLOAD_DATE QLTYW QLTYH < <(
    yt-dlp -q --no-warnings --print "${FMT}" "${URL}"
)
if [[ "$UPLOAD_DATE" =~ ^[0-9]{8}$ ]]; then
  UPLOAD_DATE_FMT="${UPLOAD_DATE:4:2}-${UPLOAD_DATE:6:2}-${UPLOAD_DATE:0:4}"
else
  UPLOAD_DATE_FMT="${UPLOAD_DATE}"
fi

OPTION_MP3LST="${VBLU}Download An Entire${RESET} \
${BCYN}MP3${RESET} \
${VBLU}Playlist${RESET}"

OPTION_BMKV="${VBLU}Download${RESET} \
${BCYN}Video${RESET} \
${VBLU}In The${RESET} \
${BCYN}Highest${RESET} \
${VBLU}Resolution${RESET}"

OPTION_B3="${VBLU}Download An${RESET} \
${BCYN}MP3${RESET} \
${VBLU}File${RESET}"

OPT_PLMP3_CMD=(
  yt-dlp
    -x --audio-format mp3
    -f ba
    -o "%(playlist_title)s/%(title)s-%(uploader)s.%(ext)s"
    --yes-playlist
    --ignore-errors
    --no-warnings
    --quiet
    --progress
    --progress-template "download: ${VBLU}DL${BCYN}:%(progress._percent_str)s ${BMGT}ETA${BCYN}: %(progress._eta_str)s ${VBLU}Speed${BCYN}:%(progress._speed_str)s"
    "$URL"
)

OPT_BMP3_CMD=(
  yt-dlp
    -x --audio-format mp3
    -f ba
    -o "%(title)s-%(uploader)s.%(ext)s"
    --quiet
    --progress
    --ignore-errors
    --console-title
    --no-warnings
    --no-check-formats
    --progress-template "download: ${VBLU}DL${BCYN}:%(progress._percent_str)s ${BMGT}ETA${BCYN}: %(progress._eta_str)s ${VBLU}Speed${BCYN}:%(progress._speed_str)s"
    "$URL"
)

OPT_BMKV_CMD=(
  yt-dlp
    -f "bestvideo+bestaudio"
    -o "%(title)s-%(uploader)s_[%(height)sp]-{Best_Quality}.%(ext)s"
    --progress
    --quiet
    --no-playlist
    --console-title
    --no-check-formats
    --no-warnings
    --ignore-errors
    --write-subs
    --embed-thumbnail
    --embed-chapters
    --embed-subs
    --sub-format srt
    --convert-subs srt
    --merge-output-format mkv
    --progress-template "download: ${VBLU}DL${BCYN}:%(progress._percent_str)s ${BMGT}ETA${BCYN}: %(progress._eta_str)s ${VBLU}Speed${BCYN}:%(progress._speed_str)s"
    "$URL"
)

DEFAULT="3"
clear
sleep 1.0
printf "\n\n"
printf "   ${VBLU}â”Œâ”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”\n"
printf "   ${VBLU}â”‚ ${BMGT}[${BCYN}1${BMGT}] %s	${VBLU}â”‚\n" "$OPTION_MP3LST"
printf "   ${VBLU}â”œâ”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”¤\n"
printf "   ${VBLU}â”‚ ${BMGT}[${BCYN}5${BMGT}] %s	${VBLU}â”‚\n" "$OPTION_B3"
printf "   ${VBLU}â”œâ”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”¤\n"
printf "   ${VBLU}â”‚ ${BMGT}[${BCYN}2${BMGT}] %s	${VBLU}â”‚\n" "$OPTION_BMKV"
printf "   ${VBLU}â”œâ”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”¤\n"
printf "   ${VBLU}â”‚ ${BMGT}[${BCYN}E${BMGT}] ${BCYN}Press ${BWHT}[${NONG}Enter${BWHT}] ${BCYN}For ${BMGT}Default ${BCYN}}Option [%d] ${VBLU}â”‚\n" "$DEFAULT"
printf "   ${VBLU}â””â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”˜\n\n\n"

printf "   ${VBLU}â”Œâ”€â”€${BCYN}Press${VBLU}â”€${ACID}Any${VBLU}â”€${BCYN}Button${VBLU}â”€${BCYN}To${VBLU}â”€${BCYN}Move${VBLU}â”€${BCYN}On${VBLU}â”€â”€â”\n"
printf "   ${VBLU}â””â”€â”€${BMGT}>${ACID}:"
read -r -t 100 DECISION
clear

DECISION=${DECISION:-$DEFAULT}
rc=2
case "${DECISION}" in
  1)
echo '
   â”€â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ•—â•”â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ•—â”€â–ˆâ–ˆâ–ˆâ•—â”€â”€â”€â–ˆâ–ˆâ–ˆâ•—â”€â–ˆâ–ˆâ•—â”€â”€â–ˆâ–ˆâ•—â•”â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ•—â”€â•”â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ•—â–ˆâ–ˆâ•—â”€â”€â–ˆâ–ˆâ•—â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ•—â”€
   â”€â–ˆâ–ˆâ•”â•â•â•â•â•â–ˆâ–ˆâ•”â•â•â–ˆâ–ˆâ•—â–ˆâ–ˆâ–ˆâ–ˆâ•—â”€â–ˆâ–ˆâ–ˆâ–ˆâ•‘â”€â–ˆâ–ˆâ•‘â”€â”€â–ˆâ–ˆâ•‘â–ˆâ–ˆâ•”â•â•â–ˆâ–ˆâ•—â–ˆâ–ˆâ•”â•â•â•â•â•â–ˆâ–ˆâ•‘â”€â–ˆâ–ˆâ•”â•â–ˆâ–ˆâ•”â•â•â–ˆâ–ˆâ•—
   â”€â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ•—â”€â”€â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ•‘â–ˆâ–ˆâ•”â–ˆâ–ˆâ–ˆâ–ˆâ•”â–ˆâ–ˆâ•‘â”€â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ•‘â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ•‘â–ˆâ–ˆâ•‘â”€â”€â”€â”€â”€â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ•”â•â”€â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ•”â•
